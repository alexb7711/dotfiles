#!/bin/sh

alias aliasrc="nvim $HOME/.config/aliasrc"
alias cm="catkin_make -DPYTHON_EXECUTABLE=/usr/bin/python -DPYTHON_INCLUDE_DIR=/usr/include/python3.9"
alias g="git"
alias gc="gcalcli --config-folder ~/.config/gcalcli/"
alias gdb="gdb -tui"
alias haros="haros --config ~/.config/haros/config.yaml --home ~/.config/haros/haros/"
alias mail="neomutt"
alias nb="newsboat"
alias pm="pulsemixer"
alias rm="trash-put"
alias sp="source $HOME/.profile"
alias spot="ncspot"
alias sz="source $HOME/.config/zsh/.zshrc"
alias v="nvim"
alias vimrc="nvim $HOME/.config/nvim/init.vim"
alias vp="nvim $HOME/.profile"
alias vs="nvim -S .vs"
alias vw="nvim -c VimwikiIndex"
alias weather="curl http://wttr.in"
alias z="zathura"
alias zshrc="nvim ~/.config/zsh/.zshrc"
